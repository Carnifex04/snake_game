<html>
    <head>
        <title>Snake Game</title>
        <link rel="stylesheet" href="style.css">
    </head>
    <body onload="loadPlayers()">
        <p hidden id="lowestScore"><%=lowestScore%></p>
        <div class="game">
            <span> Use Arrow Keys to control <span>
            <canvas id="snake" width="608" height="608"></canvas>
        </div>
        <div class="leader-back">
            <div class="leader-header">Top Scorers</div>
            <div id="leaderboard"></div>    
        </div>
    </body>
    <script src="snake.js"></script>
</html>